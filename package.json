{
    "name": "geoloc-util",
    "version": "1.0.0",
    "description": "A command-line utility to fetch geolocation data (latitude, longitude, and place name) from OpenWeather's Geocoding API for given city/state or ZIP code inputs.",
    "main": "src/index.js",
    "scripts": {
      "start": "node src/index.js",
      "test": "node tests/test.js",
      "lint": "eslint src/ tests/",
      "prettify": "prettier --write src/**/*.js tests/**/*.js"
    },
    "keywords": [
      "geolocation",
      "OpenWeather",
      "command-line-tool",
      "nodejs",
      "cli"
    ],
    "author": "rayanne",
    "dependencies": {
      "axios": "^1.5.0"
    },
    "devDependencies": {
      "eslint": "^8.53.0",
      "prettier": "^3.0.0"
    },
    "engines": {
      "node": ">=14.0.0"
    }
  }
  